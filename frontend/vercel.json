{
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"installCommand": "npm install",
	"env": {
		"VITE_API_URL": "@vite_api_url"
	},
	"headers": [
		{
			"source": "/api/:path*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-cache, no-store, must-revalidate"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/api/:path*",
			"destination": "${VITE_API_URL}/api/:path*"
		}
	]
}
